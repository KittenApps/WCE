{
  "name": "wce",
  "type": "module",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "start": "esbuild src/index.ts --bundle --outfile=public/wce.js --format=esm --sourcemap --watch --serve=4000 --servedir=public --define:PUBLIC_URL=\\'http://localhost:4000\\'",
    "build": "rollup -c",
    "test": "npm run typecheck ; npm run lint",
    "typecheck": "tsc",
    "lint": "eslint ./src",
    "serve": "npm run build && npx http-server ./dist -p 4000 -c-1 --cors"
  },
  "dependencies": {
    "buttplug": "^3.2.2",
    "dexie": "^4.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@stylistic/eslint-plugin": "^2.8.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/events": "^3.0.3",
    "@types/node": "^22.5.5",
    "bc-stubs": "^108.0.0-Beta.1",
    "bondage-club-mod-sdk": "^1.2.0",
    "esbuild": "^0.23.1",
    "eslint": "^9.10.0",
    "globals": "^15.9.0",
    "rollup": "^4.21.3",
    "rollup-plugin-esbuild": "^6.1.1",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.5.0",
    "ws": "^8.18.0"
  },
  "overrides": {
    "bc-stubs": {
      "socket.io-client": {
        "engine.io-client": {
          "ws": "$ws"
        }
      }
    }
  }
}
