{
  "name": "wce",
  "type": "module",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "start": "esbuild src/index.ts --bundle --outfile=public/wce.js --format=esm --sourcemap --watch --serve=4000 --servedir=public --define:PUBLIC_URL=\\'http://localhost:4000\\'",
    "build": "rollup -c",
    "test": "npm run typecheck ; npm run lint",
    "typecheck": "tsc",
    "lint": "eslint ./src",
    "serve": "npm run build && npx http-server ./dist -p 4000 -c-1 --cors",
    "serve:bc": "npx http-server ../Bondage-College/BondageClub -p 3000 -c-1 --cors"
  },
  "dependencies": {
    "buttplug": "^3.2.2",
    "dexie": "^4.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@stylistic/eslint-plugin": "^2.11.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/events": "^3.0.3",
    "@types/node": "^22.10.0",
    "bc-stubs": "^110.0.2",
    "bondage-club-mod-sdk": "^1.2.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "rollup": "^4.27.4",
    "rollup-plugin-esbuild": "^6.1.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0",
    "ws": "^8.18.0"
  },
  "overrides": {
    "bc-stubs": {
      "socket.io-client": {
        "engine.io-client": {
          "ws": "$ws"
        }
      }
    }
  }
}
