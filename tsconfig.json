{
	"include": [
		"node_modules/bc-stubs/bc/**/*.d.ts",
		"types/wce.d.ts",
		"src/**/*.js",
		"src/**/*.ts"
	],
	"exclude": [
		"dist/**"
	],
	"compilerOptions": {
		"module": "ES2022",
		"allowJs": true,
		"moduleResolution": "Bundler",
		"noEmit": true,
		"target": "ES2022",
		"checkJs": true,
		"skipLibCheck": false,
		"strictNullChecks": false
	}
}
